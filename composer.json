{
    "name": "10degrees/10degrees-base",
    "description": "10 Degrees base WordPress theme",
    "type": "wordpress-theme",
    "homepage": "https://github.com/10degrees/10degrees-base",
    "keywords": [
        "wordpress",
        "theme"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "jgrossi/corcel": "^5.0",
        "composer/installers": "^1.9",
        "illuminate/translation": "^8.13",
        "illuminate/validation": "^8.13",
        "johnbillion/extended-cpts": "^4.4",
        "illuminate/filesystem": "^8.13",
        "illuminate/http": "^8.13",
        "erusev/parsedown": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
