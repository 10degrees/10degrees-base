{
    "name": "10degrees/10degrees-base",
    "description": "10 Degrees base WordPress theme",
    "type": "wordpress-theme",
    "homepage": "https://github.com/10degrees/10degrees-base",
    "keywords": [
      "wordpress",
      "theme"
    ],
    "license": "MIT",
    "require": {
        "composer/installers": "^1.4",
        "nesbot/carbon": "^1.0",
        "a5hleyrich/wp-queue": "^1.3",
        "google/apiclient": "^2.0",
        "johnbillion/extended-cpts": "^4.2",
        "illuminate/filesystem": "^5.8",
        "illuminate/translation": "^5.8",
        "illuminate/validation": "^5.8"
    },
    "autoload": {
        "psr-4": {"App\\": "app/"}
    },
    "scripts": {
        "post-install-cmd": [
            "chmod +x .change.sh",
            "echo 'Now run .change.sh script to change theme name and textdomain'"
      ]
    }
}
