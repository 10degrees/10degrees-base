{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "description": "Simple front-end boilerplate using Gulp 4",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecodercoder/frontend-boilerplate.git"
  },
  "keywords": [
    "gulp",
    "boilerplate",
    "html",
    "css",
    "javascript"
  ],
  "author": "@thecodercoder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thecodercoder/frontend-boilerplate/issues"
  },
  "scripts": {
    "development": "cross-env NODE_OPTIONS=--openssl-legacy-provider mix",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider mix",
    "watch": "cross-env NODE_OPTIONS=--openssl-legacy-provider mix watch",
    "watch-poll": "cross-env mix watch -- --watch-options-poll=1000",
    "hot": "cross-env NODE_OPTIONS=--openssl-legacy-provider mix watch --hot",
    "production": "cross-env NODE_OPTIONS=--openssl-legacy-provider mix --production"
  },
  "homepage": "https://github.com/thecodercoder/frontend-boilerplate#readme",
  "devDependencies": {
    "@wordpress/babel-preset-default": "^4.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "sass": "^1.27.0",
    "sass-loader": "^8.0.2"
  },
  "dependencies": {
    "@tinypixelco/laravel-mix-wp-blocks": "^1.1.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^2.8.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.9.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "laravel-mix": "^6.0.19",
    "laravel-mix-clean": "^0.1.0",
    "laravel-mix-copy-watched": "^2.2.4",
    "laravel-mix-versionhash": "^2.0.1",
    "wp-pot": "^1.9.6"
  }
}
